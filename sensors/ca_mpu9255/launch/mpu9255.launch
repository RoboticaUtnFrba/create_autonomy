<launch>
	<!-- WiringPi needs superuser permissions -->
	<node pkg="ca_mpu9255" name="mpu9255_node" type="mpu9255_node" launch-prefix="sudo" required="true"/>

	<node pkg="imu_complementary_filter" type="complementary_filter_node" name="complementary_filter_node" output="screen">
		<param name="fixed_frame" value="base_link"/>
		<param name="use_mag" value="true"/>
		<param name="do_bias_estimation" value="true"/>
		<param name="do_adaptive_gain" value="true"/>
		<param name="gain_acc" value="0.01"/>
		<param name="gain_mag" value="0.01"/>
 		<param name="publish_tf" value="true"/>
	</node>

</launch>
